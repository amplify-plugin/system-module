{
    "name": "amplify/system",
    "description": "This is a System module for amplify",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Hafijul Islam",
            "email": "hafijul.islam@easyask.com",
            "role": "Developer"
        }
    ],
    "require": {
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-gd": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "ext-simplexml": "*",
        "ext-zip": "*",
        "amplify/api": "*",
        "amplify/backend": "*",
        "amplify/captcha": "*",
        "amplify/cms": "*",
        "amplify/custom-item": "*",
        "amplify/erp": "*",
        "amplify/frontend": "*",
        "amplify/marketing": "*",
        "amplify/media": "*",
        "amplify/message": "*",
        "amplify/order-rule": "*",
        "amplify/payment": "*",
        "amplify/sayt": "*",
        "amplify/ticket": "*",
        "amplify/utility": "*",
        "amplify/widget": "*",
        "backpack/permissionmanager": "^6.0.17",
        "barryvdh/laravel-dompdf": "^2.2",
        "carlos-meneses/laravel-mpdf": "^2.1.13",
        "consoletvs/charts": "^6.7",
        "cviebrock/eloquent-sluggable": "^10.0",
        "doctrine/dbal": "^3.8.6",
        "guzzlehttp/guzzle": "^7.8.1",
        "intervention/image": "^2.7.2",
        "laraflow/form": "^0.1.2",
        "laravel/framework": "^10.48.14",
        "league/flysystem": "^3.28",
        "league/flysystem-aws-s3-v3": "^3.28",
        "league/flysystem-ftp": "^3.28",
        "maatwebsite/excel": "^3.1.55",
        "owen-it/laravel-auditing": "^13.6.7",
        "spatie/laravel-backup": "^8.8.1",
        "spatie/laravel-health": "^1.34",
        "spatie/laravel-honeypot": "^4.5.2",
        "spatie/laravel-translatable": "^6.7.1",
        "spatie/ssl-certificate": "^2.6"
    },
    "autoload": {
        "psr-4": {
            "Amplify\\System\\": "./src"
        },
        "files": [
            "./src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Amplify\\System\\SystemServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "suggest": {
        "ext-pcntl": "For Linux with Ext. PCNTL enabled for parallel process."
    }
}
